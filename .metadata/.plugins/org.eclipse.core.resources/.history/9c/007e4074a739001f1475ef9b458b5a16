package basic;

public class Variable {

	public static void main(String[] args) {
		// 상수 & 아스키코드
		System.out.println('A' + 32);  // 기본은 숫자로 보여준다.
		System.out.println((char)('A' + 32));  // 문자로 보고 싶으면, 강제형변환 Cast연산
	
		System.out.println(2 + 3);
		System.out.println('2' + '3');
		System.out.println((int)'3');
		
	}

}
