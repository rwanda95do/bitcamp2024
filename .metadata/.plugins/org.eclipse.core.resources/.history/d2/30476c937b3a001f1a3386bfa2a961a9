package operator;

public class Operator05 {
	public static void main(String[] args) {
		boolean a = 25>36;
		System.out.println("a = " + a);
		System.out.println("!a = " + !a);
		
		String b = "apple";
		String c = "apple";
		String d = new String("apple");
		System.out.println("b==c : " + (b==c?"같다":"다르다"));
	}
}
